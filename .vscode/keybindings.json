// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+enter",
        "command": "todo.toggleBox",
        "when": "editorTextFocus && editorLangId == 'todo'"
    },
    {
        "key": "alt+enter",
        "command": "-todo.toggleBox",
        "when": "editorTextFocus && editorLangId == 'todo'"
    },
    {
        "key": "cmd+b",
        "command": "latex-workshop.recipes",
        "when": "editorLangId == 'latex'"
    },
    {
        "key": "ctrl+backspace",
        "command": "-deleteLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+backspace",
        "command": "deleteRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+delete",
        "command": "-deleteRight",
        "when": "textInputFocus"
    }
]